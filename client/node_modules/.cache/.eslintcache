ave location data for future use\n            userLat = data.latitude;\n            userLon = data.longitude;\n            fetch(\"https://api.tomtom.com/routing/1/calculateRoute/\" + userLat + \"%2C\" + userLon + \"%3A\" + lat + \"%2C\" + lon + \"/json?instructionsType=text&traffic=true&avoid=unpavedRoads&travelMode=car&vehicleCommercial=false&key=2qYjtOeeuEawxxQE7KUtVZQFywO4pRvN\").then(function(response){\n              if(response.ok){\n                response.json().then(function(data){\n                  let instructions=data.routes[0].guidance.instructions;                                             \n                  for (let i = 0; i < instructions.length; i++) {\n                    let stop = instructions[i].message;\n                    completeDirections.push(stop);\n                  };\n                });\n              }\n              else {\n                console.log(\"wooooooooow\")\n              }\n            });\n          });\n        }\n      });\n    } \n  })  \n  return completeDirections \n};\n\nexport default completeDirections;\n","C:\\Users\\15129\\OneDrive\\Desktop\\projects\\beer-me\\client\\src\\utils\\localStorage.js",[],"C:\\Users\\15129\\OneDrive\\Desktop\\projects\\beer-me\\client\\src\\utils\\helpers.js",[],{"ruleId":"87","severity":1,"message":"88","line":5,"column":16,"nodeType":"89","messageId":"90","endLine":5,"endColumn":24},{"ruleId":"87","severity":1,"message":"91","line":5,"column":26,"nodeType":"89","messageId":"90","endLine":5,"endColumn":32},{"ruleId":"87","severity":1,"message":"92","line":5,"column":34,"nodeType":"89","messageId":"90","endLine":5,"endColumn":38},{"ruleId":"87","severity":1,"message":"91","line":2,"column":10,"nodeType":"89","messageId":"90","endLine":2,"endColumn":16},{"ruleId":"87","severity":1,"message":"93","line":7,"column":9,"nodeType":"89","messageId":"90","endLine":7,"endColumn":20},{"ruleId":"87","severity":1,"message":"94","line":10,"column":10,"nodeType":"89","messageId":"90","endLi